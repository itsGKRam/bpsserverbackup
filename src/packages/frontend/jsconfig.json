{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "jsx": "react-jsx",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "baseUrl": ".",
        "paths": {
            "@store/*": ["./src/config/store/*"],
            "@components/*": ["./src/components/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}
